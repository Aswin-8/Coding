#include <stdio.h>
int main()
{
    char str[40];
    scanf("%s",str);
    int i=0,count=0,j=0;
    for(i=0;i<10;i++)
    {
        while(str[j] !='\0')
        {
            if(str[j] == i + '0')
                count++;
            j++;
        }
        printf("%d ",count);
        count=0;
        j=0;
    }
}
